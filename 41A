#include<stdio.h>
#include<string.h>

int main(){
    char a[101],b[101];
    scanf(" %s",a);
    scanf(" %s",b);

    int len1 = strlen(a);
    int len2 = strlen(b);
    if(len1!=len2){
        printf("NO");
        return 0;
    }

    else{
        for(int i=0;i<len1;i++){
            if(a[i]!=b[len1-i-1]){
                printf("NO");
                return 0;
            }
        }
    }

    printf("YES");

    return 0;
}
